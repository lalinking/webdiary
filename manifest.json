{
  "manifest_version": 2,
  "name": "__MSG_main_name__",
  "description": "__MSG_main_desc__",
  "version": "1.0",
  "default_locale": "zh_CN",
  "chrome_url_overrides": {
    "newtab": "/page/newtab/_.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "__MSG_main_name__ \n __MSG_group_nohistory__ \n __MSG_group_black__",
    "default_icon": "/resource/icon38.png",
    "default_popup": "/page/popup/_.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "history",
    "storage"
  ],
  "optional_permissions": [
    "https://*/",
    "http://*/"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "/resource/upgrade.js",
        "/page/google/_.js"
      ],
      "css": [
        "/page/google/_.css"
      ]
    }
  ]
}