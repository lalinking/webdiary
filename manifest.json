{
  "manifest_version": 3,
  "name": "__MSG_main_name__",
  "description": "__MSG_main_desc__",
  "version": "5.4.1",
  "icons": {
    "48": "/resource/icon48.png"
  },
  "author": "forveer2013@gmail.com",
  "default_locale": "zh_CN",
  "options_page": "/page/setting/page.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "__MSG_main_name__ \n __MSG_main_desc__",
    "default_icon": "/resource/icon48.png",
    "default_popup": "/page/main/page.html?from=act"
  },
  "minimum_chrome_version": "88",
  "permissions": [
    "bookmarks",
    "history",
    "storage",
    "favicon"
  ],
  "chrome_url_overrides": {
    "newtab": "/page/main/page.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/resource/hide.png",
        "/resource/hide-no.png",
        "/resource/incognito.png",
        "/resource/incognito-no.png",
        "/resource/remark.png",
        "/resource/delete-x.png",
        "/resource/icon48.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}